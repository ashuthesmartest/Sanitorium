<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="189.0" prefWidth="374.0" xmlns="http://javafx.com/javafx/8.0.40"
            fx:controller="com.controller.Dialog.PasswordChangeDialogController">
    <children>
        <GridPane alignment="CENTER" layoutX="29.0" layoutY="14.0" prefHeight="136.0" prefWidth="317.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="159.0" minWidth="10.0" prefWidth="125.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="252.0" minWidth="10.0" prefWidth="192.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="58.0" minHeight="10.0" prefHeight="45.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="50.0" minHeight="10.0" prefHeight="45.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label prefHeight="17.0" prefWidth="86.0" text="Old Password"/>
                <Label text="New Password" GridPane.rowIndex="1"/>
                <Label prefHeight="17.0" prefWidth="121.0" text="Repeat New Password" GridPane.rowIndex="2"/>
                <PasswordField fx:id="tfOldPass" GridPane.columnIndex="1"/>
                <PasswordField fx:id="tfNewPass" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <PasswordField fx:id="tfRepeat" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
            </children>
        </GridPane>
        <Button fx:id="btOk" layoutX="216.0" layoutY="150.0" mnemonicParsing="false" onAction="#handleOkBt"
                prefHeight="25.0" prefWidth="52.0" text="Ok"/>
        <Button fx:id="btCancel" layoutX="294.0" layoutY="150.0" mnemonicParsing="false" onAction="#handleCancelBt"
                text="Cancel"/>
    </children>
</AnchorPane>
